.body{
    background: #fff;
}

.swiper{
    height: 100%;
}
.swiper-item-content{
    display: inline-block;
    width: 650rpx;
    margin-left: 38rpx;
    margin-top: 10rpx;
    padding: 10rpx 10rpx 30rpx 10rpx;
    border: 1rpx solid #f0f0f0;
    box-shadow: 1rpx 1rpx 2rpx #e0e0e0;
}
.swiper-item-image{
    width: 650rpx;
    height: 650rpx;
}

.swiper-icon{
    height: 50rpx;
    width: 50rpx;
    vertical-align: middle;
}
.list-number{
    display: inline-block;
    line-height: 50rpx;
}

.swiper-submit-btn{
    padding: 0 50rpx;
    margin-top: 40rpx;
}

.list-add-btn{
    position: fixed;
    bottom: 38rpx;
    left: 38rpx;
    right: 38rpx;
    height: 100rpx;
    text-align: center;
    background: #09bb07;
    font-size: 36rpx;
    line-height: 100rpx;
    color: #fff;
    border-radius: 8rpx;
}

.userCenter{
    position: fixed;
    top: 10rpx;
    right: 38rpx;
    z-index: 100;
}

.userCenter-icon{
    height: 100rpx;
    width: 100rpx;
}
